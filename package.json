{
    "name": "@navikt/nav-chatbot",
    "version": "1.2.3",
    "description": "Chatbot Frida for nav.no",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/navikt/chatbot.git"
    },
    "main": "umd/@navikt/nav-chatbot.min.js",
    "types": "umd/@navikt/index.d.ts",
    "module": "es/index.js",
    "files": [
        "css",
        "es",
        "lib",
        "umd"
    ],
    "scripts": {
        "build": "npm run generate:types && nwb build-react-component --no-demo && import-path --path umd/@navikt/nav-chatbot.min.js",
        "clean": "nwb clean-module && nwb clean-demo",
        "prepublishOnly": "npm run build",
        "start": "nwb -c ./nwb.demo.config.js serve-react-demo --copy-files",
        "start:host": "nwb -c ./nwb.demo.config.js serve-react-demo --copy-files --host 0.0.0.0",
        "generate:types": "dts-generator --out src/index.d.ts --project ./ --exclude ./src/components/**/styles.ts --exclude ./node_modules/**/*.d.ts --prefix",
        "test": "nwb test-react",
        "test:coverage": "nwb test-react --coverage",
        "test:watch": "nwb test-react --server"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "js-cookie": "^2.2.1",
        "moment": "^2.27.0",
        "styled-components": "^4.3.2"
    },
    "peerDependencies": {
        "react": "16.x",
        "react-app-polyfill": "1.x"
    },
    "devDependencies": {
        "@types/js-cookie": "^2.2.6",
        "@types/react": "^16.8.24",
        "@types/react-dom": "^16.8.5",
        "@types/styled-components": "^4.1.18",
        "dts-generator": "^3.0.0",
        "import-path": "^1.0.14",
        "moment-locales-webpack-plugin": "^1.2.0",
        "nwb": "^0.23.0",
        "react": "^16.8.6",
        "react-app-polyfill": "^1.0.6",
        "react-dom": "^16.8.6",
        "svg-inline-loader": "^0.8.0",
        "ts-loader": "^6.0.4",
        "typescript": "^3.5.3"
    },
    "prettier": {
        "singleQuote": true,
        "jsxSingleQuote": true,
        "tabWidth": 4
    }
}
